{
  "name": "savasana-ai",
  "displayName": "Savasana ai",
  "version": "0.0.1",
  "description": "Code Challenge",
  "author": "javiergarcia2006@gmail.com",
  "scripts": {
    "start": "next start",
    "dev": "run-p dev:*",
    "dev:plasmo": "plasmo dev",
    "dev:next": "next dev --port 1947",
    "build": "run-s build:*",
    "build:plasmo": "plasmo build",
    "build:next": "next build",
    "package": "plasmo package"
  },
  "dependencies": {
    "next": "14.0.2",
    "plasmo": "0.84.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@plasmohq/messaging": "0.6.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.251",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "prettier": "3.0.3",
    "typescript": "5.2.2",
    "@plasmohq/rps": "1.8.7"
  },
  "manifest": {
    "permissions": [
      "activeTab",
      "tabs"
    ],
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ],
    "externally_connectable": {
      "matches": [
        "*://*.localhost.com/*"
      ]
    }
  }
